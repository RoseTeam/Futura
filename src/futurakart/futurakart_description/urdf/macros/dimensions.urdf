<?xml version="1.0" ?>
<!--
     Futurakart model dimensions
 -->
<robot name="xacro_properties" xmlns:xacro="http://ros.org/wiki/xacro">

  <!-- Wheelbase = distance between wheels in meters -->
  <xacro:property name="wheelbase" value="0.49" />

  <!-- Track = distance between front and rear axles -->
  <xacro:property name="track" value="0.99" />

  <!-- Wheel_vertical_offset = vertical distance between base_link and wheel center -->
  <xacro:property name="wheel_vertical_offset" value="0.09" />

  <!-- Vertical_offset = vertical distance between base_link and the floor -->
  <xacro:property name="vertical_offset" value="0.23" />

  <!--  -->
  <xacro:property name="footprint_vertical_offset" value="-0.0655" />

  <!-- Wheel dimensions -->
  <xacro:property name="wheel_radius" value="0.125" />
  <xacro:property name="wheel_width" value="0.085" />

  <!-- Base plane dimenstions -->
  <xacro:property name="base_length" value="1.5" />
  <xacro:property name="base_width" value="1.0" />
  <xacro:property name="base_height" value="0.02" />

  <xacro:property name="dummy_inertia" value="1e-09"/>
  <xacro:property name="mount_spacing" value="0.120" />



</robot>